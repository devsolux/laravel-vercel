{
  "name": "devsolux/laravel-vercel",
  "description": "Integration to run Laravel on Vercel",
  "keywords": [
    "laravel",
    "vercel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Baris Aydin",
      "email": "devsolux@gmail.com"
    }
  ],
  "type": "library",
  "require": {
    "php": "^7.4|^8.0|^8.1|^8.2",
    "illuminate/console": "^7.0|^8.0|^9.0|^10.0",
    "illuminate/container": "^7.0|^8.0|^9.0|^10.0",
    "illuminate/encryption": "^7.0|^8.0|^9.0|^10.0",
    "illuminate/support": "^7.0|^8.0|^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "DevSolux\\LaravelVercel\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DevSolux\\LaravelVercel\\VercelServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
